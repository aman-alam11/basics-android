<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="neu.droid.guy.coordinate_layout_parallax_demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EnterAlways" />
        <activity android:name=".EnterAlwaysCollapsed" />
        <activity android:name=".ExitUntillCollapsed" />
        <activity android:name=".Scroll" />
        <activity android:name=".ScrollAndExitUntillCollapsed" />
        <activity android:name=".Snap"></activity>
    </application>

</manifest>